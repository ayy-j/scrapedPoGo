              }
            }
          },
          "additionalProperties": false
        }
      },
      "megaRaidSchedule": {
        "type": "object",
        "description": "Mega Raid boss schedule by day",
        "additionalProperties": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "start": { "type": "string" },
