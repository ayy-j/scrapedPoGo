{
  "name": "scrapedpogo",
  "version": "1.0.0",
  "description": "Scrape LeekDuck.com for relevant information about Pokemon GO",
  "main": "index.js",
  "scripts": {
    "scrape": "node ./src/scrapers/scrape.js",
    "detailedscrape": "node ./src/scrapers/detailedscrape.js",
    "combinedetails": "node ./src/scrapers/combinedetails.js",
    "combineall": "USE_BLOB_URLS=true node ./src/scrapers/combineAll.js",
    "pipeline": "USE_BLOB_URLS=true npm run scrape && USE_BLOB_URLS=true npm run detailedscrape && USE_BLOB_URLS=true npm run combinedetails && npm run combineall",
    "validate": "node src/scripts/validate-schemas.js",
    "blob:upload": "node src/scripts/upload-images-to-blob.js"
  },
  "author": "quantNebula",
  "license": "MIT",
  "dependencies": {
    "@vercel/blob": "^2.0.1",
    "dotenv": "^17.2.3",
    "image-size": "^2.0.2",
    "jsdom": "27.4.0",
    "moment": "latest"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  }
}
