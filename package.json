{
  "name": "scrapedpogo",
  "version": "1.0.0",
  "description": "Web scraper for Pokemon GO event data",
  "main": "index.js",
  "scripts": {
    "scrape": "node ./src/scrapers/scrape.js",
    "detailedscrape": "node ./src/scrapers/detailedscrape.js",
    "combinedetails": "node ./src/scrapers/combinedetails.js",
    "combineall": "USE_BLOB_URLS=true node ./src/scrapers/combineAll.js",
    "pipeline": "USE_BLOB_URLS=true npm run scrape && USE_BLOB_URLS=true npm run detailedscrape && USE_BLOB_URLS=true npm run combinedetails && npm run combineall",
    "validate": "node src/scripts/validate-schemas.js",
    "compare:schemas": "node src/scripts/compare-docs-data-schemas.js --mode report",
    "compare:schemas:strict": "node src/scripts/compare-docs-data-schemas.js --mode strict",
    "test": "node --test",
    "blob:upload": "node src/scripts/upload-images-to-blob.js",
    "blob:repair": "node src/scripts/upload-images-to-blob.js --repair",
    "blob:repair-dry": "node src/scripts/upload-images-to-blob.js --repair --dry-run",
    "serve": "node serve.js",
    "dev": "npm run serve",
    "build": "node -e \"require('fs').cpSync('data', 'public/data', {recursive: true});\""
  },
  "author": "quantNebula",
  "license": "MIT",
  "dependencies": {
    "@vercel/blob": "^2.2.0",
    "dotenv": "^17.3.1",
    "image-size": "^2.0.2",
    "jsdom": "28.0.0",
    "moment": "latest"
  },
  "devDependencies": {
    "ajv": "^8.18.0",
    "ajv-formats": "^3.0.1",
    "sharp": "^0.34.5"
  }
}