{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": {
    "silent": true
  },
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json;charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=7200, stale-while-revalidate=3600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/pokemon/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/pokemon/:path*"
    },
    {
      "source": "/events/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/events/:path*"
    },
    {
      "source": "/bonuses/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/bonuses/:path*"
    },
    {
      "source": "/types/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/types/:path*"
    },
    {
      "source": "/items/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/items/:path*"
    },
    {
      "source": "/weather/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/weather/:path*"
    },
    {
      "source": "/misc/:path*",
      "destination": "https://btzr53b2kyiebsam.public.blob.vercel-storage.com/misc/:path*"
    }
  ]
}
